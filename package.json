{
  "name": "@aureooms/js-partition",
  "description": "Partition algorithms for JavaScript",
  "version": "8.0.0",
  "license": "AGPL-3.0",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "homepage": "https://make-github-pseudonymous-again.github.io/js-partition",
  "repository": {
    "url": "https://github.com/make-github-pseudonymous-again/js-partition",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/make-github-pseudonymous-again/js-partition/issues"
  },
  "keywords": [
    "hoare",
    "javascript",
    "js",
    "lomuto",
    "partition",
    "yaroslavskiy"
  ],
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "build-docs": "esdoc",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "esdoc": "esdoc",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "@array-like/alloc": "^0.0.1",
    "@array-like/fill": "^0.0.1",
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@babel/register": "7.16.5",
    "@functional-abstraction/functools": "^3.0.2",
    "@iterable-iterator/chain": "^2.0.1",
    "@iterable-iterator/consume": "^1.0.1",
    "@iterable-iterator/list": "^1.0.1",
    "@iterable-iterator/map": "^1.0.1",
    "@randomized/random": "^4.0.0",
    "@set-theory/cartesian-product": "^2.0.2",
    "@total-order/primitive": "^1.0.1",
    "ava": "^3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.2.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.10.0",
    "coveralls": "3.1.1",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "np": "7.6.0",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.9",
    "xo": "0.47.0"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  }
